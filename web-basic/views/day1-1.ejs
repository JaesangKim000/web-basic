                    <option>00</option>
                    <option>10</option>
                    <option>20</option>
                    <option>30</option>
                    <option>40</option>
                    <option>50</option>
                </select>분<br/>
                <input type="text" placeholder="강의실 장소를 입력하세요"><br/><br/>
                <input type="submit" />




            </form>

